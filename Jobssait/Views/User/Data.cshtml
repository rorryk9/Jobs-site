@model UserDTO

<table class="table">
    <thead>
        <tr>
            <th scope="col">Email</th>
            <th scope="col">Username</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">@Model.Email</th>
            <td>@Model.Userusername</td>
        </tr>
    </tbody>
</table>

<a asp-action="UserPosts" asp-route-id="@Model.Id" asp-controller="Post" class="btn btn-primary">See my posts</a>
